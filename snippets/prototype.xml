<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="xtended"
           fetch="https://github.com/Project-Xtended"
           revision="refs/heads/xr" />
  
  <remote  name="aosip"
           fetch="https://github.com/AOSiP"
           revision="refs/heads/eleven" />
  
  <remote  name="prototype"
           fetch="https://github.com/PrototypeOS"
           revision="refs/heads/master" />
  
  <remote  name="lighthouse-gitlab"
           fetch="https://gitlab.com/Stealth1226"
           revision="eleven" />
  
  <remote  name="pixel-gitlab"
           fetch="https://gitlab.pixelexperience.org/android"
           revision="eleven" />
  
  <remote name="superior"
          fetch="https://github.com/SuperiorOS/"
          review="gerrit.superioros.org"
          revision="refs/heads/eleven" />
  
  
  <!-- Device repos -->
  <project path="device/prototype/sepolicy" name="device_prototype_sepolicy" remote="prototype" />
  <project path="device/qcom/sepolicy-legacy-um" name="device_qcom_sepolicy-legacy-um" remote="prototype" />
  <project path="device/qcom/sepolicy_vndr" name="device_qcom_sepolicy_vndr" remote="prototype" />
  
  <!-- Packages repos -->
  <project path="packages/apps/FaceUnlockService" name="PixelExperience/packages_apps_FaceUnlockService" remote="github" revision="eleven" />
  <project path="packages/apps/Lawnchair" name="packages_apps_Lawnchair" remote="xtended" />
  <project path="packages/apps/Plugins" name="packages_apps_Plugins" remote="xtended" />
  <project path="packages/apps/ThemePicker" name="platform_packages_apps_ThemePicker" remote="aosip" />
  
  <!-- System Repos -->
  <project path="system/bt" name="android_system_bt" groups="pdk" remote="superior" />
  <project path="system/core" name="android_system_core" remote="superior" />
  <project path="system/netd" name="android_system_netd" remote="superior" />
  <project path="system/sepolicy" name="android_system_sepolicy" remote="superior" />
  <project path="system/security" name="android_system_security" remote="superior" />
  <project path="system/tools/mkbootimg" name="android_system_tools_mkbootimg" remote="superior" />
  <project path="system/update_engine" name="android_system_update_engine" groups="pdk" remote="superior" />
  <project path="system/vold" name="android_system_vold" remote="superior" />
  
  <!-- Vendor -->
  <project path="vendor/prototype" name="vendor_prototype" remote="prototype" />
  <project path="vendor/gapps" name="vendor_gapps" remote="lighthouse-gitlab" />

  
  </manifest>
