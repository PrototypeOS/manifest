<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  
  <remote  name="prototype"
           fetch="https://github.com/PrototypeOS"
           revision="refs/heads/ranch" />

   <remote  name="github"
           fetch="https://github.com/" />
  
   <remote  name="lighthouse"
           fetch="https://github.com/lighthouse-os"
           revision="raft" />
  
  <!-- Build repositories -->
  <project path="build/soong" name="build_soong" remote="lighthouse" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  <project path="bionic" name="bionic" remote="lighthouse"/>
  <project path="build/blueprint" name="build_blueprint" remote="lighthouse"/>
  <project path="bootable/recovery" name="bootable_recovery" remote="lighthouse"/>
  
  <!-- Device repos -->
  <project path="device/prototype/sepolicy" name="android_device_prototype_sepolicy" remote="prototype" />
  <project path="device/qcom/sepolicy-legacy-um" name="android_device_qcom_sepolicy-legacy-um" remote="prototype" />
  <project path="device/qcom/sepolicy_vndr" name="android_device_qcom_sepolicy_vndr" remote="prototype" />
   <project path="device/qcom/sepolicy" name="android_device_qcom_sepolicy" remote="prototype" />
 
  <project path="manifest" name="manifest" remote="prototype" />
  
   

  <!-- Vendor -->
  <project path="vendor/prototype" name="vendor_prototype" remote="prototype" />
  
  </manifest>
