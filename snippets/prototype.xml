<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  
  <remote  name="prototype"
           fetch="https://github.com/PrototypeOS"
           revision="refs/heads/ranch" />
  
   <remote  name="lighthouse"
           fetch="https://github.com/lighthouse-os"
           revision="raft" />
  
   <remote  name="tos"
           fetch="https://github.com/TitaniumOS"
           revision="refs/heads/eleven" />

  <remote name="superior"
          fetch="https://github.com/SuperiorOS/"
          review="gerrit.superioros.org"
          revision="refs/heads/eleven" />


  <project path="bionic" name="android_bionic" remote="superior" />
  <project path="bootable/recovery" name="android_bootable_recovery" remote="superior" />

  <!-- Build Repos -->
  <project path="build/make" name="android_build" groups="pdk" remote="superior" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>

  <project path="build/blueprint" name="android_build_blueprint" groups="pdk,tradefed" remote="superior" />
  <project path="build/soong" name="android_build_soong" groups="pdk,tradefed" remote="superior" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>
  
  <!-- Device repos -->
  <project path="device/prototype/sepolicy" name="android_device_prototype_sepolicy" remote="prototype" />
  <project path="device/qcom/sepolicy-legacy-um" name="android_device_qcom_sepolicy-legacy-um" remote="prototype" />
  <project path="device/qcom/sepolicy_vndr" name="android_device_qcom_sepolicy_vndr" remote="prototype" />
   <project path="device/qcom/sepolicy" name="android_device_qcom_sepolicy" remote="prototype" />
  
  <!-- Prebuilts -->
  <project path="prebuilts/apps/Lawnchair" name="prebuilts_apps_Lawnchair" clone-depth="1" remote="lighthouse" />
  <project path="prebuilts/abi-dumps/vndk" name="prebuilts_abi-dumps_vndk" clone-depth="1" remote="tos" />
  <project path="prebuilts/clang/host/linux-x86/clang-proton" name="kdrag0n/proton-clang" remote="github" revision="master" clone-depth="1" /> 
  
  
  <project path="manifest" name="manifest" remote="prototype" />
  
   

  <!-- Vendor -->
  <project path="vendor/prototype" name="vendor_prototype" remote="prototype" />
  
  </manifest>
